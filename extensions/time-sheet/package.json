{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "time-sheet",
  "title": "Time sheet",
  "description": "Automate filling timesheet",
  "icon": "command-icon.png",
  "author": "jeet0007",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Time Sheet",
      "description": "Show Today TimeSheet",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "saveDirectory",
      "type": "textfield",
      "default": "~",
      "required": true,
      "title": "Save Exports Directory",
      "description": "All Exports will be saved here"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
